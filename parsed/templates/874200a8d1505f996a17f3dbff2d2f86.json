{
  "components/cart/totals": "<div class=\"cart-totals\">\n    <div class=\"column column-1\">\n        {{#each cart.taxes}}\n            <div class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{name}}:</strong>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{cost.formatted}}</span>\n                </div>\n            </div>\n        {{/each}}\n        {{#if cart.discount }}\n            <div class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.discount'}}:</strong>\n                </div>\n                <div class=\"cart-total-value\">\n                    {{cart.discount.formatted}}\n                </div>\n            </div>\n        {{/if}}\n        {{#if cart.coupons}}\n            {{#each cart.coupons}}\n                <div class=\"cart-total\">\n                    <div class=\"cart-total-label\">\n                        <strong>{{lang 'cart.coupons.code_label' code=code}}</strong>\n                        <div>\n                            <a href=\"{{remove_url}}\">{{lang 'cart.coupons.remove'}}</a>\n                        </div>\n                    </div>\n                    <div class=\"cart-total-value\">\n                        <span>{{discount.formatted}}</span>\n                    </div>\n                </div>\n            {{/each}}\n        {{else}}\n            <div class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.coupon_code'}}:</strong>\n                </div>\n                {{> components/cart/coupon-input}}\n            </div>\n        {{/if}}\n        {{#if settings.gift_certificates_enabled}}\n            {{#each cart.gift_certificates}}\n                <div class=\"cart-total\">\n                    <div class=\"cart-total-label\">\n                        <strong>{{lang 'cart.gift_certificates.code_label' code=code}}</strong>\n                        <span>\n                            {{remaining.formatted}} | <a href=\"{{remove_url}}\">{{lang 'cart.gift_certificates.remove'}}</a>\n                        </span>\n                    </div>\n                    <div class=\"cart-total-value\">\n                        {{used.formatted}}\n                    </div>\n                </div>\n            {{/each}}\n            <div class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.gift_certificates.gift_certificate'}}:</strong>\n                </div>\n                {{> components/cart/gift-certificate-input}}\n            </div>\n        {{/if}}\n    </div>\n    <div class=\"column column-2\">\n        {{#if cart.shipping_handling.show_estimator}}\n            <div class=\"cart-total\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.checkout.shipping'}}:</strong>\n                </div>\n                {{> components/cart/shipping-estimator cart.shipping_handling}}\n            </div>\n        {{/if}}\n    </div>\n    <div class=\"column column-3\">\n        <div class=\"cart-total\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.order_title'}}:</strong>\n            </div>\n        </div>\n        <div class=\"cart-total cart-total-custom\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.subtotal'}}:</strong>\n            </div>\n            <div class=\"cart-total-value\">\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                    <span>{{cart.sub_total.formatted}}</span>\n                {{else}}\n                    {{> components/common/login-for-pricing}}\n                {{/or}}\n            </div>\n        </div>\n        {{#if cart.gift_wrapping_cost.amount}}\n            <div class=\"cart-total cart-total-custom\">\n                <div class=\"cart-total-label\">\n                    <strong>{{lang 'cart.checkout.gift_wrapping'}}:</strong>\n                </div>\n                <div class=\"cart-total-value\">\n                    <span>{{cart.gift_wrapping_cost.formatted}}</span>\n                </div>\n            </div>\n        {{/if}}\n        <div class=\"cart-total cart-total-custom cart-total-grandTotal\">\n            <div class=\"cart-total-label\">\n                <strong>{{lang 'cart.checkout.grand_total'}}:</strong>\n            </div>\n            <div class=\"cart-total-value cart-total-grandTotal\">\n                {{#or customer (if theme_settings.restrict_to_login '!==' true)}}\n                    <span>{{cart.grand_total.formatted}}</span>\n                {{else}}\n                    {{> components/common/login-for-pricing}}\n                {{/or}}\n            </div>\n        </div>\n        {{#if cart.show_primary_checkout_button}}\n            <div class=\"cart-actions\">\n                <a class=\"button button--primary\" href=\"{{urls.checkout.single_address}}\" title=\"{{lang 'cart.checkout.title'}}\">{{lang 'cart.checkout.button'}}</a>\n                {{#if cart.show_multiple_address_shipping}}\n                    <a class=\"checkoutMultiple\" href=\"{{urls.checkout.multiple_address}}\">\n                        {{lang 'cart.preview.checkout_multiple'}}\n                    </a>\n                {{/if}}\n            </div>\n        {{else}}\n            <div class=\"cart-actions\">\n                <a class=\"button\" href=\"{{urls.home}}\" title=\"{{lang 'cart.continue_shopping'}}\">{{lang 'cart.continue_shopping'}}</a>\n            </div>\n        {{/if}}\n        {{#if cart.additional_checkout_buttons}}\n            <div class=\"cart-additionalCheckoutButtons\">\n                {{#each cart.additional_checkout_buttons}}\n                    {{{this}}}\n                {{/each}}\n            </div>\n        {{/if}}\n        <div class=\"themevale_securityContact\">\n            <div class=\"wrapper\">\n                <i class=\"icon\"><svg><use xlink:href=\"#icon-user-protection\"></use></svg></i>\n                <p class=\"text\">{{lang 'cart.checkout.contact_title'}}</p>\n            </div>\n            <p class=\"description\">{{lang 'cart.checkout.contact_content'}}</p>\n        </div>\n    </div>\n</div>\n",
  "components/cart/coupon-input": "<div class=\"cart-form coupon-code\">\n\t<p class=\"text\">{{lang 'cart.coupons.coupon_des'}}</p>\n    <form class=\"form form--hiddenLabels coupon-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"couponcode\">{{lang 'cart.coupons.coupon_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.coupons.empty_error'}}\"  id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupons.coupon_code'}}\">\n        <input class=\"button button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n",
  "components/cart/gift-certificate-input": "<div class=\"cart-form gift-certificate-code\">\n\t<p class=\"text\">{{lang 'cart.gift_certificates.gift_des'}}</p>\n    <form class=\"form form--hiddenLabels cart-gift-certificate-form\" method=\"post\" action=\"{{urls.cart}}\">\n        <label class=\"form-label\" for=\"certcode\">{{lang 'cart.gift_certificates.cert_code'}}</label>\n        <input class=\"form-input\" data-error=\"{{lang 'cart.gift_certificates.empty_error'}}\"  id=\"certcode\" type=\"text\" name=\"certcode\" value=\"\" placeholder=\"{{lang 'cart.gift_certificates.add_cert_code'}}\">\n        <input class=\"button button--small\" type=\"submit\" value=\"{{lang 'cart.coupons.button'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n</div>\n",
  "components/common/login-for-pricing": "<p translate>Log in for pricing</p>\n",
  "components/cart/shipping-estimator": "<div class=\"shipping-estimator\">\n    <p class=\"text\">{{lang 'cart.shipping_estimator.shipping_des'}}</p>\n    <form class=\"form estimator-form\" data-shipping-estimator>\n        <dl>\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\n                    {{#each countries}}\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                            {{name}}\n                        </option>\n                    {{/each}}\n                </select>\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\n            </dt>\n\n            <dd class=\"estimator-form-input\">\n                {{#if states.length}}\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\n                        {{#each states}}\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                                {{name}}\n                            </option>\n                        {{/each}}\n                    </select>\n                {{else}}\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\n                {{/if}}\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\n            </dd>\n\n            <dt class=\"estimator-form-label\">\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\n            </dt>\n            <dd class=\"estimator-form-input\">\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\n            </dd>\n            <button class=\"button button--small shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\n        </dl>\n    </form>\n    <div class=\"shipping-quotes\"></div>\n</div>\n"
}
