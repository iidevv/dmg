  "components/themevale/homepage/component/category-banner": "<section class=\"themevale_section themevale_category-banner padding-top-100 padding-bottom-100\">\r\n    <div class=\"themevale_category-background\">\r\n        <div class=\"title-wrapper\">\r\n            <h3 class=\"label\">{{lang 'products.badges.new'}}</h3>\r\n            {{#each categories}}\r\n                {{#if id '==' ../theme_settings.themevale_category-banner-id}}\r\n                    <h2 class=\"page-heading\"><a href=\"{{url}}\">{{name}}</a></h2>\r\n                {{/if}}\r\n            {{/each}}\r\n        </div>\r\n        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.themevale_category-banner-image}}\" alt=\"\">\r\n    </div>\r\n    <div class=\"container\">\r\n        <div class=\"themevale_category-content\">\r\n            <div class=\"items themevale_category-products\">\r\n                <div class=\"themevale_category-product-item\" style=\"top: {{theme_settings.themevale_category-banner-product1-y}}%; left: {{theme_settings.themevale_category-banner-product1-x}}%;\">\r\n                    <div class=\"position-point\" data-product-id=\"{{theme_settings.themevale_category-banner-product1-id}}\">+</div>\r\n                </div>\r\n                <div class=\"themevale_category-product-item\" style=\"top: {{theme_settings.themevale_category-banner-product2-y}}%; left: {{theme_settings.themevale_category-banner-product2-x}}%;\">\r\n                    <div class=\"position-point\" data-product-id=\"{{theme_settings.themevale_category-banner-product2-id}}\">+</div>\r\n                </div>\r\n            </div>\r\n            <div class=\"items themevale_category-id\">\r\n                {{#each categories}}\r\n                    {{#if id '==' ../theme_settings.themevale_category-banner-id}}\r\n                        <h2 class=\"page-heading\"><a href=\"{{url}}\">{{name}}</a></h2>\r\n                   