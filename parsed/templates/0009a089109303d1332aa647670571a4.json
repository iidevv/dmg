{
  "components/products/filter": "<form class=\"actionBar\" method=\"get\" data-sort-by data-show-products>\n    <fieldset class=\"form-fieldset actionBar-section\">\n        <div class=\"form-field\">\n            <label class=\"form-label\" for=\"limit\">{{lang 'common.sorter.show'}}</label>\n            <select class=\"form-select\" name=\"limit\" id=\"limit\">\n                <option value=\"9\" {{#if '9' '==' products_per_page }}selected{{/if}}>9 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"12\" {{#if '12' '==' products_per_page }}selected{{/if}}>12 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"15\" {{#if '15' '==' products_per_page }}selected{{/if}}>15 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"20\" {{#if '20' '==' products_per_page }}selected{{/if}}>20 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"24\" {{#if '24' '==' products_per_page }}selected{{/if}}>24 {{lang 'common.sorter.items_per_page'}}</option>\n                <option value=\"30\" {{#if '30' '==' products_per_page }}selected{{/if}}>30 {{lang 'common.sorter.items_per_page'}}</option>\n            </select>\n        </div>\n    </fieldset>\n    <script>\n(function(){\n    try {\n        var url = new URL(window.location.href);\n        var c = url.searchParams.get(\"limit\");\n        if(c != null){\n            var limit = document.querySelectorAll('select#limit option');\n            Array.prototype.forEach.call(limit, function(element) {\n                if(element.value == c){\n                    element.selected = true;\n                }\n            });\n        }\n    } catch(e) {}\n}());\n</script>\n</form>\n<form class=\"actionBar\" method=\"get\" data-sort-by>\n    {{> components/category/sort-box sort=sort}}\n</form>\n",
  "components/category/sort-box": "<fieldset class=\"form-fieldset actionBar-section\">\n    <div class=\"form-field\">\n        <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\n        <select class=\"form-select\" name=\"sort\" id=\"sort\">\n            <option value=\"featured\" {{#if sort '==' 'featured'}}selected{{/if}}>{{lang 'common.sorter.featured'}}</option>\n            <option value=\"newest\" {{#if sort '==' 'newest'}}selected{{/if}}>{{lang 'common.sorter.newest'}}</option>\n            <option value=\"bestselling\" {{#if sort '==' 'bestselling'}}selected{{/if}}>{{lang 'common.sorter.top_sellers'}}</option>\n            <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\n            <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            <option value=\"avgcustomerreview\" {{#if sort '==' 'avgcustomerreview'}}selected{{/if}}>{{lang 'common.sorter.reviews'}}</option>\n            <option value=\"priceasc\" {{#if sort '==' 'priceasc'}}selected{{/if}}>{{lang 'common.sorter.price_asc'}}</option>\n            <option value=\"pricedesc\" {{#if sort '==' 'pricedesc'}}selected{{/if}}>{{lang 'common.sorter.price_desc'}}</option>\n            {{#if result_count}}\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\n            {{/if}}\n        </select>\n    </div>\n</fieldset>\n\n"
}
