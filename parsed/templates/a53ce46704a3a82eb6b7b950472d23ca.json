{
  "components/carousel": "<section class=\"themevale_heroCarousel\">\n    <div class=\"heroCarousel\"\n        data-slick='{\n            \"dots\": false,\n            \"arrows\": false,\n            \"mobileFirst\": true,\n            \"slidesToShow\": 1,\n            \"slidesToScroll\": 1,\n            \"adaptiveHeight\": true,\n            \"lazyLoad\": \"anticipated\",\n            \"autoplaySpeed\": 7000,\n            \"autoplay\": true,\n            \"fade\": true,\n            \"nextArrow\": \"<svg class=&apos;slick-next slick-arrow&apos;><use xlink:href=&apos;#icon-slick-next&apos;></use></svg>\", \n            \"prevArrow\": \"<svg class=&apos;slick-prev slick-arrow&apos;><use xlink:href=&apos;#icon-slick-prev&apos;></use></svg>\",\n            \"responsive\": [\n                {\n                    \"breakpoint\": 1100,\n                    \"settings\": {\n                        \"dots\": {{dots}},\n                        \"arrows\": {{arrows}}\n                    }\n                }\n        ]}'>\n        {{#each carousel.slides}}\n        <div class=\"heroCarousel-slide {{#if ../theme_settings.homepage_stretch_carousel_images}}stretch{{/if}} {{#if @first}}heroCarousel-slide--first{{/if}}\">\n            <div class=\"heroCarousel-image-wrapper\" {{#and image_height image_width}}style=\"height: {{multiply (divide image_height image_width) 100}}vw\"{{/and}}>\n                <a class=\"link\" href=\"{{url}}\"></a>\n                {{#and (if @index '==' 0) ../theme_settings.homepage_images_mobile-0}}\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-0}}\" alt=\"\">\n                {{/and}}\n                {{#and (if @index '==' 1) ../theme_settings.homepage_images_mobile-1}}\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-1}}\" alt=\"\">\n                {{/and}}\n                {{#and (if @index '==' 2) ../theme_settings.homepage_images_mobile-2}}\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-2}}\" alt=\"\">\n                {{/and}}\n                {{#and (if @index '==' 3) ../theme_settings.homepage_images_mobile-3}}\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-3}}\" alt=\"\">\n                {{/and}}\n                {{#and (if @index '==' 4) ../theme_settings.homepage_images_mobile-4}}\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-4}}\" alt=\"\">\n                {{/and}}\n                <img class=\"heroCarousel-image\" {{#if @first}}src{{else}}src=\"{{cdn 'img/loading.svg'}}\" data-lazy{{/if}}=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"\n                {{#if image_width}}width=\"{{image_width}}\"{{/if}} {{#if image_height}}height=\"{{image_height}}\"{{/if}} />\n            </div>\n            {{#if heading}}\n                {{> components/carousel-content show_background=true}}\n            {{else}}\n                {{#if text}}\n                    {{> components/carousel-content show_background=true}}\n                {{else}}\n                    {{#if button_text}}\n                        {{> components/carousel-content show_background=false}}\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n        </div>\n        {{/each}}\n    </div>\n</section>\n",
  "components/carousel-content": "<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    <h1 class=\"heroCarousel-title animated\">{{{heading}}}</h1>\n    <p class=\"heroCarousel-description animated\">{{{text}}}</p>\n    {{#if button_text}}\n        <a class=\"heroCarousel-action button button--large animated\" href=\"{{url}}\">{{button_text}}</a>\n    {{/if}}\n</div>\n"
}
