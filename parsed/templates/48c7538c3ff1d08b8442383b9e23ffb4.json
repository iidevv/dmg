{
  "components/themevale/homepage/component/ajax-products-by-category-sorting-tabs-4-content": "<ul class=\"tabs\" data-tab data-themevale-products-by-category-sorting-tabs role=\"tablist\">\r\n    <li class=\"tab is-active\" role=\"presentation\">\r\n        <a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-new-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" aria-controls=\"tab-productsByCategoryTabs-new-{{id}}\">{{lang 'home.tab_products.new' }}</a>\r\n    </li>\r\n    <li class=\"tab\" role=\"presentation\">\r\n        <a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-bestselling-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"true\" aria-controls=\"tab-productsByCategoryTabs-bestselling-{{id}}\">{{lang 'home.tab_products.bestselling' }}</a>\r\n    </li>\r\n    <li class=\"tab\" role=\"presentation\">\r\n        <a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-featured-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" aria-controls=\"tab-productsByCategoryTabs-featured-{{id}}\">{{lang 'home.tab_products.featured' }}</a>\r\n    </li>\r\n    <li class=\"tab\" role=\"presentation\">\r\n        <a class=\"tab-title\" href=\"#tab-productsByCategoryTabs-reviews-{{id}}\" role=\"tab\" tabindex=\"0\" aria-selected=\"false\" aria-controls=\"tab-productsByCategoryTabs-reviews-{{id}}\">{{lang 'home.tab_products.reviews' }}</a>\r\n    </li>\r\n</ul>\r\n<div class=\"tabs-contents\">\r\n    <div class=\"themevale_marketingBanner-wrapper\">\r\n        <div class=\"themevale_marketingBanner\"\r\n            data-slick='{\r\n                \"dots\": true,\r\n                \"arrows\": false,\r\n                \"slidesToShow\": 1,\r\n                \"slidesToScroll\": 1,\r\n                \"autoplay\": true,\r\n                \"infinite\": true,\r\n                \"autoplaySpeed\": 3000\r\n            }'>\r\n            {{#if image-1}}\r\n            <div class=\"item\">\r\n                <div class=\"themevale_marketingBanner-image\">\r\n                    <a href=\"{{link-1}}\" class=\"link\">\r\n                        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{image-1}}\" alt=\"\">\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            {{/if}}\r\n            {{#if image-2}}\r\n            <div class=\"item\">\r\n                <div class=\"themevale_marketingBanner-image\">\r\n                    <a href=\"{{link-2}}\" class=\"link\">\r\n                        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{image-2}}\" alt=\"\">\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            {{/if}}\r\n            {{#if image-3}}\r\n            <div class=\"item\">\r\n                <div class=\"themevale_marketingBanner-image\">\r\n                    <a href=\"{{link-3}}\" class=\"link\">\r\n                        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{image-3}}\" alt=\"\">\r\n                    </a>\r\n                </div>\r\n            </div>\r\n            {{/if}}\r\n        </div>\r\n        <div class=\"themevale_marketingBanner-subcategory\">\r\n            <a href=\"{{url}}\" class=\"title-category\">{{name}}</a>\r\n            {{#each children}}\r\n                <div class=\"sub-category\">\r\n                    <a class=\"link\" href=\"{{url}}\">{{name}}</a>\r\n                </div>\r\n            {{/each}}\r\n            <div class=\"group-button\">\r\n                <a class=\"button button--primary\" href=\"{{url}}\">{{lang 'common.shop_now'}}</a>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div role=\"tabpanel\" aria-hidden=\"true\" class=\"tab-content is-active\" id=\"tab-productsByCategoryTabs-new-{{id}}\" \r\n        data-themevale-products-by-category-sorting-tabs=\"{{concat url '?sort=newest'}}\"\r\n        data-themevale-template=\"themevale/homepage/component/ajax-products-by-category-sorting-tabs-result-2\">\r\n        {{>  components/themevale/homepage/component/product-loading}}\r\n    </div>\r\n\r\n    <div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content\" id=\"tab-productsByCategoryTabs-bestselling-{{id}}\" \r\n        data-themevale-products-by-category-sorting-tabs=\"{{concat url '?sort=bestselling'}}\"\r\n        data-themevale-template=\"themevale/homepage/component/ajax-products-by-category-sorting-tabs-result-2\">\r\n        {{>  components/themevale/homepage/component/product-loading}}\r\n    </div>\r\n\r\n    <div role=\"tabpanel\" aria-hidden=\"true\" class=\"tab-content\" id=\"tab-productsByCategoryTabs-featured-{{id}}\" \r\n        data-themevale-products-by-category-sorting-tabs=\"{{concat url '?sort=featured'}}\"\r\n        data-themevale-template=\"themevale/homepage/component/ajax-products-by-category-sorting-tabs-result-2\">\r\n        {{>  components/themevale/homepage/component/product-loading}}\r\n    </div>\r\n\r\n    <div role=\"tabpanel\" aria-hidden=\"true\" class=\"tab-content\" id=\"tab-productsByCategoryTabs-reviews-{{id}}\" \r\n        data-themevale-products-by-category-sorting-tabs=\"{{concat url '?sort=avgcustomerreview'}}\"\r\n        data-themevale-template=\"themevale/homepage/component/ajax-products-by-category-sorting-tabs-result-2\">\r\n        {{>  components/themevale/homepage/component/product-loading}}\r\n    </div>\r\n</div>\n",
  "components/themevale/homepage/component/product-loading": "<div class=\"themevale_productLoading text-center\">\r\n    <img src=\"{{cdn 'img/loading.svg'}}\" alt=\"loading\" title=\"loading\" />\r\n</div>\r\n"
}
