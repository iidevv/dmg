{
  "components/themevale/homepage/component/ajax-products-by-category-sorting-new": "<section class=\"themevale_section padding-top-50 padding-bottom-50\" id=\"themevale_new_product\">\r\n    <div class=\"container\">\r\n        <script type=\"text/javascript\">\r\n            var arrNew = [0{{#each products}},{{id}}{{/each}}];\r\n        </script>\r\n        <div class=\"themevale_productsCategorySortNew\">\r\n            <h2 class=\"page-heading text-center\">{{lang 'products.new' }}</h2>\r\n            <ul class=\"tabs\" data-tab role=\"tablist\">\r\n                {{#each (limit categories theme_settings.themevale_tab-product-category-limit)}}\r\n                <li class=\"tab {{#if @index '==' 0}}is-active{{/if}}\" role=\"presentation\" cat-id=\"{{id}}\" cat-url=\"{{url}}\">\r\n                    <a class=\"tab-title\" href=\"#tab-{{id}}\" role=\"tab\" tabindex=\"0\">{{name}}</a>\r\n                    <script> \r\n                        var url_{{id}} = '{{url}}';\r\n                        url_{{id}} = url_{{id}}.replace('http:', location.protocol);\r\n                        if(document.querySelectorAll('a[cat-id=\"{{id}}\"]')[0] != undefined)\r\n                            document.querySelectorAll('a[cat-id=\"{{id}}\"]')[0].setAttribute(\"cat-url\", url_{{id}});\r\n                    </script>\r\n                </li>\r\n                {{/each}}\r\n            </ul>\r\n            <div class=\"tabs-contents\">\r\n                {{#each (limit categories theme_settings.themevale_tab-product-category-limit)}}\r\n                <div role=\"tabpanel\" aria-hidden=\"false\" class=\"tab-content {{#if @index '==' 0}}is-active{{/if}}\" id=\"tab-{{id}}\">\r\n                    {{> components/themevale/homepage/component/product-loading}}\r\n                </div>\r\n                {{/each}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n</section>\r\n{{{region name=\"below_products_by_category\"}}}\n",
  "components/themevale/homepage/component/product-loading": "<div class=\"themevale_productLoading text-center\">\r\n    <img src=\"{{cdn 'img/loading.svg'}}\" alt=\"loading\" title=\"loading\" />\r\n</div>\r\n"
}
