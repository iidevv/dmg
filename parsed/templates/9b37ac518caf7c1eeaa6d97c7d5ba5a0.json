{
  "components/carousel-2": "<section class=\"themevale_heroCarousel\">\r\n    <div class=\"heroCarousel\"\r\n        data-slick='{\r\n            \"dots\": false,\r\n            \"arrows\": false,\r\n            \"mobileFirst\": true,\r\n            \"slidesToShow\": 1,\r\n            \"slidesToScroll\": 1,\r\n            \"adaptiveHeight\": true,\r\n            \"lazyLoad\": \"anticipated\",\r\n            \"autoplaySpeed\": 7000,\r\n            \"autoplay\": true,\r\n            \"fade\": true,\r\n            \"nextArrow\": \"<svg class=&apos;slick-next slick-arrow&apos;><use xlink:href=&apos;#icon-slick-next&apos;></use></svg>\", \r\n            \"prevArrow\": \"<svg class=&apos;slick-prev slick-arrow&apos;><use xlink:href=&apos;#icon-slick-prev&apos;></use></svg>\",\r\n            \"responsive\": [\r\n                {\r\n                    \"breakpoint\": 1100,\r\n                    \"settings\": {\r\n                        \"dots\": {{dots}},\r\n                        \"arrows\": {{arrows}}\r\n                    }\r\n                }\r\n        ]}'>\r\n        {{#each carousel.slides}}\r\n        <div class=\"heroCarousel-slide {{#if ../theme_settings.homepage_stretch_carousel_images}}stretch{{/if}} {{#if @first}}heroCarousel-slide--first{{/if}}\">\r\n            <div class=\"heroCarousel-image-wrapper\">\r\n                <a class=\"link\" href=\"{{url}}\"></a>\r\n                {{#and (if @index '==' 0) ../theme_settings.homepage_images_mobile-0}}\r\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-0}}\" alt=\"\">\r\n                {{/and}}\r\n                {{#and (if @index '==' 1) ../theme_settings.homepage_images_mobile-1}}\r\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-1}}\" alt=\"\">\r\n                {{/and}}\r\n                {{#and (if @index '==' 2) ../theme_settings.homepage_images_mobile-2}}\r\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-2}}\" alt=\"\">\r\n                {{/and}}\r\n                {{#and (if @index '==' 3) ../theme_settings.homepage_images_mobile-3}}\r\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-3}}\" alt=\"\">\r\n                {{/and}}\r\n                {{#and (if @index '==' 4) ../theme_settings.homepage_images_mobile-4}}\r\n                    <img class=\"heroCarousel-image-mobile lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{../../theme_settings.homepage_images_mobile-4}}\" alt=\"\">\r\n                {{/and}}\r\n                <img class=\"heroCarousel-image\" {{#if @first}}src{{else}}src=\"{{cdn 'img/loading.svg'}}\" data-lazy{{/if}}=\"{{image}}\" alt=\"{{alt_text}}\" title=\"{{alt_text}}\"\r\n                {{#if image_width}}width=\"{{image_width}}\"{{/if}} {{#if image_height}}height=\"{{image_height}}\"{{/if}} />\r\n            </div>\r\n            {{#if heading}}\r\n                {{> components/carousel-content show_background=true}}\r\n            {{else}}\r\n                {{#if text}}\r\n                    {{> components/carousel-content show_background=true}}\r\n                {{else}}\r\n                    {{#if button_text}}\r\n                        {{> components/carousel-content show_background=false}}\r\n                    {{/if}}\r\n                {{/if}}\r\n            {{/if}}\r\n        </div>\r\n        {{/each}}\r\n    </div>\r\n</section>\n",
  "components/carousel-content": "<div class=\"heroCarousel-content{{#unless show_background}} heroCarousel-content--empty{{/unless}}\">\n    <h1 class=\"heroCarousel-title animated\">{{{heading}}}</h1>\n    <p class=\"heroCarousel-description animated\">{{{text}}}</p>\n    {{#if button_text}}\n        <a class=\"heroCarousel-action button button--large animated\" href=\"{{url}}\">{{button_text}}</a>\n    {{/if}}\n</div>\n"
}
