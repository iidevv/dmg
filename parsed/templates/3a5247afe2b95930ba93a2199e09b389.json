{
  "components/themevale/newsletter": "<div id=\"themevale_newsletter\" class=\"hide\">\r\n    <div class=\"popup-overlay\"></div>\r\n    <div class=\"newsletterPopup\">\r\n        <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\" data-close-newsletter-popup>\r\n            <span aria-hidden=\"true\">&#215;</span>\r\n        </a>\r\n        <div class=\"newsletter-content\">\r\n            <h2 class=\"newsletter-heading\">{{ theme_settings.newsletterPopup-title}}</h2>\r\n            <p class=\"newsletter-sub-heading\">{{ theme_settings.newsletterPopup-subtitle}}</p>\r\n            <div id=\"popupSubcribeForm\">\r\n                <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"popupSubcribeFormSubmit\">\r\n                    <fieldset class=\"form-fieldset\">\r\n                        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n                        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n                        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n                        <div class=\"form-field\">\r\n                            <div class=\"form-wrap\">\r\n                                <input class=\"form-input\" id=\"nl_email2\" name=\"nl_email\" type=\"email\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                                <input class=\"button button--primary\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n                            </div>\r\n                        </div>\r\n                    </fieldset>\r\n                </form>\r\n            </div>\r\n            <p class=\"newsletter-message\">{{ theme_settings.newsletterPopup-content}}</p>\r\n            <div class=\"newsletter-icons\">\r\n                {{> components/common/social-links}}\r\n            </div>\r\n            <div class=\"newsletter-show-again\">\r\n                <form>\r\n                    <input id=\"do-not-show-again\" class=\"do-not-show-again\" type=\"checkbox\" name=\"don't show again\" value=\"1\"> \r\n                    <label for=\"do-not-show-again\">{{lang 'newsletter.dont_show'}}</label>\r\n                </form>\r\n            </div>\r\n        </div>\r\n        <div class=\"newsletter-image\">\r\n            <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{ theme_settings.newsletterPopup-image}}\" alt=\"\">\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                {{#if display_name '===' 'google+'}}\n                    <a class=\"icon icon--google\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <svg><use xlink:href=\"#icon-google\" /></svg>\n                    </a>\n                {{else}}\n                    <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                        <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                    </a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n"
}
