{
  "components/themevale/category/subcategories": "{{#if category.subcategories}}\r\n    <div class=\"themevale_subCategory\">\r\n        <div class=\"container\">\r\n            <div class=\"themevale_column themevale_customColumn themevale_column--col3\">\r\n                {{#each category.subcategories}}\r\n                    <div class=\"item\">\r\n                        <div class=\"sub-category\">\r\n                            <div class=\"content\">\r\n                                <a class=\"title\" href=\"{{url}}\">\r\n                                    <span class=\"text\">{{name}}</span><span class=\"count\">{{product_count}}</span>\r\n                                </a>\r\n                                {{#if description}}\r\n                                <div class=\"description\">\r\n                                    {{{description}}}\r\n                                </div>\r\n                                {{else}}\r\n                                <div class=\"description\">\r\n                                    <p>Lorem ipsum dolor sit amet consectetur adipiscing elit. Nam convallis imperdiet leo faucibus pulvinar justo tempus</p>\r\n                                </div>\r\n                                {{/if}}\r\n                            </div>\r\n                            <div class=\"image\">\r\n                                <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"{{getImage image 'themevale_subCategory_image_size' (cdn ../theme_settings.default_image_category)}}\" alt=\"{{name}}\">\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                {{/each}}\r\n            </div>\r\n        </div>\r\n    </div>\r\n{{/if}} \r\n"
}
