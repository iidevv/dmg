{
  "components/themevale/homepage/component/custom_recent_blogs": "<section class=\"themevale_section themevale_section_custom_blogs padding-top-70 padding-bottom-70 background-gray\">\r\n    <div class=\"container\">\r\n        <div class=\"items\">\r\n            {{#if blog.recent_posts}}\r\n                {{> components/themevale/homepage/component/recent_blogs-2 blog=blog.recent_posts}}\r\n            {{/if}}\r\n        </div>\r\n        <div class=\"items\">\r\n            <div class=\"page-heading-wrapper text-left\">\r\n                <h2 class=\"page-heading\">{{theme_settings.homepage_video_title}}</h2>\r\n                <a class=\"page-heading-link\" href=\"/blog/\">{{lang 'common.view_all'}}</a>\r\n            </div>\r\n            <div class=\"themevale_video_carousel\" data-slick='{\r\n                \"dots\": true,\r\n                \"arrows\": false,\r\n                \"infinite\": false,\r\n                \"rows\": 2,\r\n                \"slidesToScroll\": 1,\r\n                \"slidesToShow\": 1\r\n            }'>\r\n                <div class=\"items\">\r\n                    <div class=\"video-wrapper\">\r\n                        <div class=\"video-image\">\r\n                            <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.homepage_video_image_1}}\" alt=\"\">\r\n                            <img class=\"lazyload icon-video\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-wia9gbrvyj/product_images/uploaded_images/icon-video.png\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"video-content\">\r\n                            <div class=\"title\">{{theme_settings.homepage_video_title_1}}</div>\r\n                            <div class=\"description\">{{theme_settings.homepage_video_description_1}}</div>\r\n                        </div>\r\n                        <a href=\"javascript:void(0)\" class=\"link-video\" {{#if theme_settings.homepage_video_url_1}} data-reveal-id=\"popup-video-1\" {{/if}}></a>\r\n                    </div>\r\n                </div>\r\n                <div class=\"items\">\r\n                    <div class=\"video-wrapper\">\r\n                        <div class=\"video-image\">\r\n                            <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.homepage_video_image_2}}\" alt=\"\">\r\n                            <img class=\"lazyload icon-video\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-wia9gbrvyj/product_images/uploaded_images/icon-video.png\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"video-content\">\r\n                            <div class=\"title\">{{theme_settings.homepage_video_title_2}}</div>\r\n                            <div class=\"description\">{{theme_settings.homepage_video_description_2}}</div>\r\n                        </div>\r\n                        <a href=\"javascript:void(0)\" class=\"link-video\" {{#if theme_settings.homepage_video_url_2}} data-reveal-id=\"popup-video-2\" {{/if}}></a>\r\n                    </div>\r\n                </div>\r\n                <div class=\"items\">\r\n                    <div class=\"video-wrapper\">\r\n                        <div class=\"video-image\">\r\n                            <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.homepage_video_image_3}}\" alt=\"\">\r\n                            <img class=\"lazyload icon-video\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-wia9gbrvyj/product_images/uploaded_images/icon-video.png\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"video-content\">\r\n                            <div class=\"title\">{{theme_settings.homepage_video_title_3}}</div>\r\n                            <div class=\"description\">{{theme_settings.homepage_video_description_3}}</div>\r\n                        </div>\r\n                        <a href=\"javascript:void(0)\" class=\"link-video\" {{#if theme_settings.homepage_video_url_3}} data-reveal-id=\"popup-video-3\" {{/if}}></a>\r\n                    </div>\r\n                </div>\r\n                <div class=\"items\">\r\n                    <div class=\"video-wrapper\">\r\n                        <div class=\"video-image\">\r\n                            <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.homepage_video_image_4}}\" alt=\"\">\r\n                            <img class=\"lazyload icon-video\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-wia9gbrvyj/product_images/uploaded_images/icon-video.png\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"video-content\">\r\n                            <div class=\"title\">{{theme_settings.homepage_video_title_4}}</div>\r\n                            <div class=\"description\">{{theme_settings.homepage_video_description_4}}</div>\r\n                        </div>\r\n                        <a href=\"javascript:void(0)\" class=\"link-video\" {{#if theme_settings.homepage_video_url_4}} data-reveal-id=\"popup-video-4\" {{/if}}></a>\r\n                    </div>\r\n                </div>\r\n                <div class=\"items\">\r\n                    <div class=\"video-wrapper\">\r\n                        <div class=\"video-image\">\r\n                            <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"/product_images/uploaded_images/{{theme_settings.homepage_video_image_5}}\" alt=\"\">\r\n                            <img class=\"lazyload icon-video\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-wia9gbrvyj/product_images/uploaded_images/icon-video.png\" alt=\"\">\r\n                        </div>\r\n                        <div class=\"video-content\">\r\n                            <div class=\"title\">{{theme_settings.homepage_video_title_5}}</div>\r\n                            <div class=\"description\">{{theme_settings.homepage_video_description_5}}</div>\r\n                        </div>\r\n                        <a href=\"javascript:void(0)\" class=\"link-video\" {{#if theme_settings.homepage_video_url_5}} data-reveal-id=\"popup-video-5\" {{/if}}></a>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n    <div id=\"popup-video-1\" class=\"modal themevale_popup-video\" data-reveal>\r\n    </div>\r\n    <div id=\"popup-video-2\" class=\"modal themevale_popup-video\" data-reveal>\r\n    </div>\r\n    <div id=\"popup-video-3\" class=\"modal themevale_popup-video\" data-reveal>\r\n    </div>\r\n    <div id=\"popup-video-4\" class=\"modal themevale_popup-video\" data-reveal>\r\n    </div>\r\n    <div id=\"popup-video-5\" class=\"modal themevale_popup-video\" data-reveal>\r\n    </div>\r\n</section>\r\n{{{region name=\"below_custom_recent_blogs\"}}}\n",
  "components/themevale/homepage/component/recent_blogs-2": "{{#if theme_settings.homepage_blog_title}}\r\n    <div class=\"page-heading-wrapper text-left\">\r\n        <h2 class=\"page-heading\">{{theme_settings.homepage_blog_title}}</h2>\r\n        <a class=\"page-heading-link\" href=\"/blog/\">{{lang 'common.view_all'}}</a>\r\n    </div>\r\n{{/if}}\r\n<div class=\"recent_post_list\">\r\n    {{#each blog}}\r\n        <div class=\"items\">\r\n            {{> components/blog/recent_post post=this settings=../settings readmore=../theme_settings.homepage_blog_showreadmore}}\r\n        </div>\r\n    {{/each}}\r\n</div>\r\n{{{region name=\"below_recent_blogs\"}}}\n",
  "components/blog/recent_post": "<article class=\"blog recent-post\">\r\n    <div class=\"blog-post-figure\">\r\n        {{#if post.thumbnail}}\r\n            <figure class=\"blog-thumbnail\">\r\n                <a href=\"{{post.url}}\">\r\n                    <img src=\"{{getImage post.thumbnail 'homepage_blog_image_size'}}\" alt=\"{{post.thumbnail.alt}}\" title=\"{{post.thumbnail.alt}}\">\r\n                </a>\r\n            </figure>\r\n        {{/if}}\r\n    </div>\r\n    <div class=\"blog-post-body\">\r\n        <div class=\"blog-header\">\r\n            <h2 class=\"blog-title\">\r\n                <a href=\"{{post.url}}\">{{post.title}}</a>\r\n            </h2>\r\n            <p class=\"blog-date\">{{#if post.author}}{{lang 'blog.posted_by' name=post.author}} on {{/if}}{{post.date_published}}</p>\r\n        </div>\r\n        <div class=\"blog-post\">\r\n            {{{post.summary}}}\r\n            {{#if post.show_read_more}}\r\n                {{#if readmore '==' true}}\r\n                <br>\r\n                <a href=\"{{url}}\">read more <svg class=\"icon\"><use xlink:href=\"#icon-slick-next\"></use></svg></a>\r\n                {{/if}}\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n</article>\n"
}
